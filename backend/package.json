{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "rm -rf dist && npx --yes tsc --project tsconfig.json",
    "start": "node dist/index.js",
    "migrate": "tsx src/migrate.ts",
    "test": "vitest run",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org motion-mavericks-pty-ltd --project motion-backend ./dist && sentry-cli sourcemaps upload --org motion-mavericks-pty-ltd --project motion-backend ./dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.687.0",
    "@aws-sdk/s3-request-presigner": "^3.687.0",
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/rate-limit": "^8.1.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@sentry/cli": "^2.53.0",
    "@sentry/node": "^10.8.0",
    "@sentry/profiling-node": "^10.8.0",
    "@types/jsonwebtoken": "^9.0.10",
    "argon2": "^0.44.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.5",
    "typescript": "^5.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8",
    "tsx": "^4.19.2",
    "vitest": "^3.2.4"
  }
}
